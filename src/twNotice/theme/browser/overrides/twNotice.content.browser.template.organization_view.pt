<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="twNotice.content">
<body>


<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core"
                    tal:define="toc context/table_of_contents|nothing;">



<section class="content">
      <metal:macro use-macro="context/@@theme_macro/macros/adv_search" />
      <!--
      Main Content start
      //-->
      <section class="envor-section">
        <div class="container">
          <div class="row">
            <div class="col-lg-3">
<!-- 插入 portlet (左) -->
              <tal:block replace="structure provider:plone.leftcolumn" />
            </div>
            <!--
            Content Section start
            //-->
            <div class="col-lg-9">


<h3 tal:content="context/title"/>

<h5 tal:content="context/description"/>

<h5 tal:content="context/address"/>
<h5 tal:content="context/orgCode"/>

<h5 tal:content="context/pccOrgCode"/>

<h5>
  <strong i18n:translate="">Parent Organization</strong>:
  <a tal:attributes="href context/parentOrg/to_object/absolute_url |nothing"
     tal:content="context/parentOrg/to_object/title | nothing" />
</h5>

<h5>
  <strong i18n:translate="">Old Organization</strong>:
  <a tal:attributes="href context/oldOrg/to_object/absolute_url | nothing"
     tal:content="context/oldOrg/to_object/title | nothing" />
</h5>

<h5>
  <strong i18n:translate="">New Organization</strong>:
  <a tal:attributes="href context/newOrg/to_object/absolute_url | nothing"
     tal:content="context/newOrg/to_object/title | nothing" />
</h5>



            </div>
          </div>
        </div>
      <!--
      Main Content start
      //-->
      </section>




      <metal:macro use-macro="context/@@theme_macro/macros/call_to_action_1" />
</section>






</metal:content-core>
</metal:content-core>

</body>
</html>


