<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="twNotice.content">
<body>


<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core"
                    tal:define="toc context/table_of_contents|nothing;">

<section class="content">
      <metal:macro use-macro="context/@@theme_macro/macros/adv_search" />
      <!--
      Main Content start
      //-->
      <section class="envor-section">
        <div class="container">
          <div class="row">
            <div class="col-lg-3">
<!-- 插入 portlet (左) -->
              <tal:block replace="structure provider:plone.leftcolumn" />
            </div>
            <!--
            Content Section start
            //-->
            <div class="col-lg-9">
<h3 tal:content="context/title"/>

<h5 tal:content="context/description"/>

<h5>
 <strong i18n:translate="">Date String</strong>:
 <span tal:replace="context/dateString"/>
</h5>

<h5>
 <strong i18n:translate="">Related CPC</strong>:
 <a tal:attributes="href context/cpc/to_object/absolute_url"
    tal:content="context/cpc/to_object/title"/>
</h5>

<h5>
 <strong i18n:translate="">Notice Type</strong>:
 <span tal:replace="context/noticeType"/>
</h5>

<h5>
 <strong i18n:translate="">Notice URL</strong>:
 <a tal:attributes="href context/noticeURL"
    tal:content="context/noticeURL" target="_blank"/>
</h5>

<h5><strong i18n:translate="">Notice Metadata</strong>:</h5>
<ul>
  <li tal:repeat="key context/noticeMeta/keys">
    <strong tal:content="key"/>:
    <span tal:replace="structure python:context.noticeMeta[key]"/>
  </li>
</ul>


            </div>
          </div>
        </div>
      <!--
      Main Content start
      //-->
      </section>



            <!--
            * Counter
            //-->
            <div class="riva-countdown" id="riva-countdown">
              <!--
              * Days
              //-->
              <div class="riva-countdown-item" id="riva-countdown-days">
                <div class="value"><p>0</p></div>
                <div class="label">days</div>
              </div>
              <!--
              * Hours
              //-->
              <div class="riva-countdown-item" id="riva-countdown-hours">
                <div class="value"><p>0</p></div>
                <div class="label">hours</div>
              </div>
              <!--
              * Minutes
              //-->
              <div class="riva-countdown-item" id="riva-countdown-minutes">
                <div class="value"><p>0</p></div>
                <div class="label">minutes</div>
              </div>
              <!--
              * seconds
              //-->
              <div class="riva-countdown-item" id="riva-countdown-seconds">
                <div class="value"><p>0</p></div>
                <div class="label">seconds</div>
              </div>
            </div>



      <metal:macro use-macro="context/@@theme_macro/macros/call_to_action_1" />
</section>



</metal:content-core>
</metal:content-core>

</body>
</html>


